pragma solidity ^ 0.8.11;

contract GreetRekhabc {
    string public  name;
    string public GreetRekhabcPrefix = "Hello";
    constructor (string memory Initialname) {
        name= Initialname;
    }

    function setname(string memory newname)
    public {name= newname;}
    function getGreetRekhabc()public view returns (string memory )
    {
        return string (abi.encodePacked(GreetRekhabcPrefix, name));
    }
}

